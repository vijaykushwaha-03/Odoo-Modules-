<odoo>
    <record id="view_user_password_log_tree" model="ir.ui.view">
        <field name="name">user.password.log.tree</field>
        <field name="model">user.password.log</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list string="User Password Log" create="false">
                <field name="user_id"/>
                <field name="changed_by"/>
                <field name="change_date"/>
            </list>
        </field>
    </record>
    <record id="action_user_password_log" model="ir.actions.act_window">
        <field name="name">Password Change Logs</field>
        <field name="res_model">user.password.log</field>
        <field name="view_mode">list</field>
    </record>

    <menuitem id="menu_user_password_log"
        name="Password Logs"
        parent="base.menu_administration"
        action="action_user_password_log"/>
</odoo>