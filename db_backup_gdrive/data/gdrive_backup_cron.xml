<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="wt_gdrive_db_backup_cron" model="ir.cron">
            <field name="name">Schedule Google Drive Backup</field>
            <field name="model_id" ref="model_gdrive_backup"/>
            <field name="state">code</field>
            <field name="code">model.create_gdrive_backup()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>           
        </record>
    </data>
</odoo>
