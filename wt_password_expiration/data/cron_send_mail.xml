<odoo>
    <data noupdate="1">
        <record id="ir_cron_password_expiry_alert" model="ir.cron">
            <field name="name">Send Password Expiry Alerts</field>
            <field name="model_id" ref="wt_password_expiration.model_res_users_password_expiry"/>
            <field name="state">code</field>
            <field name="code">model.action_send_expiry_reminder()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <!-- <field name="numbercall">-1</field> -->
            <field name="active">True</field>
        </record>
    </data>
</odoo>
