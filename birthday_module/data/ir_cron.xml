<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="wt_scheduled_bday_emails_cron" model="ir.cron">
            <field name="name">Scheduled  Birthday Emails Wishes</field>
            <field name="model_id" ref="model_bday_email"/>
            <field name="state">code</field>
            <field name="code">model._bday_email_wishes()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>  
            <field name="nextcall" eval="(datetime.now().replace(hour=6, minute=31, second=0)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>
    </data>
</odoo>
 